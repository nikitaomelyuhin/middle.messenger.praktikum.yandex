{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint/eslint-plugin"
  ],
  "extends": [
    "airbnb/base",
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "no-plusplus": "off",
    "eol-last": "off",
    "max-len": "off",
    "quotes": ["error", "double"],
    "no-alert": "off"
  }
} 