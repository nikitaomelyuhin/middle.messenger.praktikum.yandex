form: translate(-50%, -50%);\n}\n\n.avatar-user__icon {\n  opacity: 1;\n  pointer-events: none;\n  position: absolute;\n  transition: .3s;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  font-size: 70px;\n  color: $text;\n}\n\n.avatar-user__image {\n  width: 100%;\n  height: 100%;\n  flex-shrink: 0;\n  background-color: $bg-input;\n}\n\n.avatar-user__pic {\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n  object-position: top center;\n}\n","../src/partials/appButton.scss":".app-button {\n  display: flex;\n  padding: 15px 10px;\n  border-radius: 7px;\n  border: 1px solid transparent;\n  background-color: $primary;\n  width: 100%;\n  justify-content: center;\n  transition: .3s;\n  &:hover {\n    background-color: $primary-hover;\n    & .app-button__link {\n      color: #fff;\n    }\n  }\n}\n\n.app-button__link {\n  color: #fff;\n}\n\n.app-button__text {\n  color: #fff;\n}\n\n.simple {\n  padding: 0;\n  border-radius: none;\n  border: none;\n  background: none;\n  font-weight: bold;\n  width: fit-content;\n  & .app-button__text {\n    color: $primary;\n  }\n  &:hover {\n    background: none;\n    & .app-button__text {\n      color: $primary-hover;\n    }\n  }\n}\n\n.danger {\n  & .app-button__link {\n    color: $danger;\n  }\n  &:hover {\n    & .app-button__link {\n      color: $danger-hover;\n    }\n  }\n}","../src/pages/userCard/userCard.scss":"@import \"../../layout/variables.scss\";\n@import \"../../layout/back.scss\";\n@import \"../../layout/avatar-user.scss\";\n@import \"../../partials/appButton.scss\";\n\n.user-card {\n  height: 100%;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.user-card__body {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  & .user-card__list:not(:last-child) {\n    margin-bottom: 40px;\n  }\n}\n\n.user-card__avatar {\n  width: 200px;\n  height: 200px;\n  cursor: pointer;\n  margin-bottom: 20px;\n}\n\n.user-card__name {\n  margin-bottom: 60px;\n}\n\n.user-card__list {\n  width: 100%;\n}\n\n.user-card__item {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding-bottom: 5px;\n  border-bottom: 1px solid $border-light;\n  margin-bottom: 15px;\n  &:last-child {\n    border-bottom: none;\n    margin-bottom: 0;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"c05ce46bba73f4f361d0ab9a16c6ffd6","cacheData":{"env":{}}}